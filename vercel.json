{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "MICROCMS_API_KEY": "${MICROCMS_API_KEY}",
    "MICROCMS_SERVICE_DOMAIN": "${MICROCMS_SERVICE_DOMAIN}",
    "NEXT_PUBLIC_MICROCMS_API_KEY": "${NEXT_PUBLIC_MICROCMS_API_KEY}",
    "NEXT_PUBLIC_MICROCMS_SERVICE_DOMAIN": "${NEXT_PUBLIC_MICROCMS_SERVICE_DOMAIN}",
    "NEXT_PUBLIC_GOOGLE_ADSENSE_ID": "${NEXT_PUBLIC_GOOGLE_ADSENSE_ID}"
  }
}
